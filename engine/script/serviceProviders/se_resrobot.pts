<?xml version="1.0" encoding="UTF-8"?>
<serviceProvider fileVersion="1.0" version="2.0" type="script">
    <name lang="en">Resrobot</name>
    <description lang="de">Dieser Service-Provider liefert Ergebnisse für alle Städte in Schweden.</description>
    <description lang="en">This service provider works for all cities in Sweden.</description>
    <author>
        <fullname>Friedrich Pülz</fullname>
        <short>fpuelz</short>
        <email>fpuelz@gmx.de</email>
    </author>
    <url>http://www.resrobot.se</url>
    <shortUrl>www.resrobot.se</shortUrl>
    <script extensions="qt.core,qt.xml">se_resrobot.js</script>
    <changelog>
        <entry version="1.0" engineVersion="0.10">Initial version.</entry>
        <entry version="1.1">Update to new script API, new stop suggestion URL, data in JSON format, like for de_db</entry>
        <entry version="2.0">Use base_hafas.js, shorter data sources, stops by geo position</entry>
    </changelog>
    <samples>
        <stop>Stockholm Centralstation</stop>
        <stop>Scheelegatan (Stockholm kn)</stop>
        <longitude>18.05</longitude>
        <latitude>59.3333</latitude>
    </samples>
</serviceProvider>
