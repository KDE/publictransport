<?xml version="1.0" encoding="UTF-8"?>
<serviceProvider fileVersion="1.1" version="2.0" type="script">
    <name lang="en">Rhein-Main-Verkehrsverbund GmbH</name>
    <description lang="en">Provides departure information for 'Rhine-Main' in Germany. It uses an XML source for departure information and HTML for stop autocompletion.</description>
    <author>
        <fullname>Friedrich Pülz</fullname>
        <short>fpuelz</short>
        <email>fpuelz@gmx.de</email>
    </author>
    <url>http://www.rmv.de/</url>
    <shortUrl>www.rmv.de</shortUrl>
    <script extensions="qt.core,qt.xml">de_rmv.js</script>
    <cities>
        <city>Frankfurt (Main)</city>
        <city>Langen (Hessen)</city>
        <city>Köln</city>
        <city>Mainz</city>
    </cities>
    <changelog>
        <entry version="2.0">Use base_hafas.js, shorter data sources, stops by geo position</entry>
        <entry version="1.4" engineVersion="0.10">Port to a script using qt.xml extension (almost copy'n'paste).</entry>
        <entry version="1.3" engineVersion="0.10">Fix departures/arrivals (also without stop ID, decoding).</entry>
        <entry version="1.3" engineVersion="0.10">Fix stop suggestions (changed HTML).</entry>
        <entry version="1.2" engineVersion="0.10">Ported the stop suggestion part to a script.</entry>
        <entry version="1.1" engineVersion="0.7">Stop suggestions didn't work, because the HTML accessor that parses stop suggestions for XML accessors was initialized with empty accessor info.</entry>
        <entry version="1.1" engineVersion="0.7">Corrected the feature list.</entry>
    </changelog>
    <samples>
        <stop>Frankfurt (Main) Zoo</stop>
        <stop>Frankfurt (Main) Hausen</stop>
        <longitude>8.68056</longitude>
        <latitude>50.1167</latitude>
    </samples>
</serviceProvider>
